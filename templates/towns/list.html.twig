{% extends 'base.html.twig' %}

{% block title %}Towns
{% endblock %}

{% block body %}


	<h1>Towns list</h1>
	<table class="table">
		<thead>
			<tr>
				<th>Name</th>
				<th>State</th>
			</tr>
		</thead>
        <tbody>
            {% for town in towns %}
            <tr>
                <td>{{town.name}}</td>
				<td>{{town.state}}</td>
            </tr>
            {% endfor %}
        </tbody>
	</table>
{% endblock %}
